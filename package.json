{
  "name": "winston-slack-webhook-transport-ts",
  "version": "1.2.6",
  "description": "A Slack transport for Winston 3 that logs to a channel via webhooks. With enabling inject Proxy Agent",
  "main": "./cjs/index.cjs",
  "types": "./index.d.ts",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build:esm": "tsc -p tsconfig.mjs.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./cjs/index.d.cts",
        "default": "./cjs/index.cjs"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeogyuGim/winston-slack-webhook-transport-ts.git"
  },
  "author": "Seogyugim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeogyuGim/winston-slack-webhook-transport-ts/issues"
  },
  "homepage": "https://github.com/SeogyuGim/winston-slack-webhook-transport-ts#readme",
  "files": [
    "cjs/index.cjs",
    "cjs/index.d.cts",
    "esm/index.js",
    "esm/index.d.ts",
    "index.d.ts",
    "index.ts",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.1.3",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.8.2",
    "babel-plugin-rewire": "^1.2.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
